<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Shopping Suite" parallel="classes" thread-count="3">

    <listeners>
        <listener class-name="utilities.TestListener"/>
    </listeners>

    <test name="All Tests">
        <parameter name="browser" value="chrome"/>
        <packages>
            <package name="testCases"/>
        </packages>
    </test>

</suite>
